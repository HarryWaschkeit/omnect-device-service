{
    "default-ratings": [
	{
	    "condition": ".exitstatus == \"SEGV\" or .exitstatus == \"ABRT\"",
	    "rating": "red"
	},
	{
	    "condition": ".exitstatus == \"HUP\"",
	    "rating": "yellow"
	},
	{
	    "condition": ".exitstatus == \"0\"",
	    "rating": "green"
	}
    ],
    "services": [
	{
	    "service": "omnect-device-service"
	},
	{
	    "service": "aziot-certd"
	},
	{
	    "service": "aziot-edged"
	},
	{
	    "service": "aziot-identityd"
	},
	{
	    "service": "aziot-keyd"
	},
	{
	    "service": "aziot-tpmd"
	},
	{
	    "service": "deviceupdate-agent"
	}
    ]
}
